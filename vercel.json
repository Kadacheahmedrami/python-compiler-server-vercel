{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/logic.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/logic",
        "dest": "api/logic.py"
      },
      {
        "src": "/(.*)",
        "methods": ["POST"],
        "dest": "api/index.js"
      }
    ]
  }